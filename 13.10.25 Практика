TASK 1 NUM4
#include <iostream>
using namespace std;
int main(){
    int a,b,x;
    char c;
    cout << "Введите числа";
    cin >> a >> b;
    cout << "Операцию";
    cin >> c;
    if (c=='+') {
        x=a+b;
        cout << x;
    }else if(c=='-'){
        x=a-b;
        cout << x;
    }else if(c=='*'){
        x=a*b;
        cout << x;
    }else{
        x=a/b;
        cout << x;
    }
}
TASK 2 NUM5
#include <iostream>
using namespace std;
int main(){
    int a,b;
    cout << "Введите числа";
    cin >> a >> b;
    while(a>b){
        if((a%2==0) and (a/2>=b)){
            a=a/2;
            cout <<":2\n";
        }else{
            a=a-1;
            cout << "-1\n";
        }
    }
}
