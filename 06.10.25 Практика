// Заполнение массива 
#include <iostream>
using namespace std;
int main() {
    int a [3];
    for(int i=0 ;i<3;i++){
        cin >> a[i];
    }
    for(int i=0 ;i<3;i++){
        cout << a[i] <<' ';
    }

    return 0;
}


#include <iostream>
#include <ctime>
using namespace std;
int main() {
    srand(time(0));
    int h;
    cout << "Введите значение:";
    cin >> h;
    int a [h];
    for(int i=0 ;i<h;i++){
        a[i]=rand()%20 - rand()%20;
    }
    for(int i=0 ;i<h;i++){
        cout << a[i] <<' ';
    }

    return 0;
}


#include <iostream>
#include <ctime>
using namespace std;
int main() {
    srand(time(0));
    int h=3,k=3;
    cout << "Введите значения:"<< "\n";
    int a [h][k];
    for(int i=0 ;i<h;i++){
        for(int i1=0 ;i1<k;i1++){
            a[i][i1]=rand()%20 - rand()%20;
        }
    }
    for(int i=0 ;i<h;i++){
        for(int i1=0 ;i1<k;i1++){
            cout << a[i][i1] <<" ";
        }
        cout<< endl;
    }

    return 0;
}
